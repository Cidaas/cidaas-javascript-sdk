{
  "name": "cidaas-javascript-sdk",
  "version": "2.2.7",
  "author": "cidaas by Widas ID GmbH",
  "description": "Cidaas native javascript sdk",
  "license": "MIT",
  "main": "src/main/index.js",
  "types": "types/main/index.d.ts",
  "engine": {
    "node": ">=8.9.10"
  },
  "scripts": {
    "dev": "webpack --config webpack.dev.js",
    "build": "npm run build-types && webpack --config webpack.prod.js",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage",
    "build-types": "npx -p typescript tsc src/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "files": [
    "src",
    "types",
    "README.md",
    "license.MD",
    "Changelogs.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@types/node": "^18.11.18",
    "crypto-js": "^4.1.1",
    "oidc-client": "^1.11.5"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^9.0.0",
    "jest": "27.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "typescript": "^4.5.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/main/**/*.{js,jsx,mjs}"
    ]
  }
}
